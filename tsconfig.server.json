{
  "compilerOptions": {
    "outDir": "dist/server", // ビルド出力先
    "rootDir": "server", // エントリーフォルダ
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["server"]
}
